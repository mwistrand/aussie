{
  "serviceId": "demo-service",
  "displayName": "Demo Service",
  "baseUrl": "http://demo:3000",
  "defaultVisibility": "PRIVATE",
  "visibilityRules": [
    {
      "pattern": "/api/health",
      "methods": ["GET"],
      "visibility": "PUBLIC"
    },
    {
      "pattern": "/api/users",
      "methods": ["GET", "POST"],
      "visibility": "PUBLIC"
    },
    {
      "pattern": "/api/auth/test",
      "methods": ["GET", "POST"],
      "visibility": "PUBLIC"
    },
    {
      "pattern": "/login",
      "methods": ["GET"],
      "visibility": "PUBLIC"
    },
    {
      "pattern": "/api/auth/login",
      "methods": ["POST"],
      "visibility": "PUBLIC"
    },
    {
      "pattern": "/api/auth/callback",
      "methods": ["GET"],
      "visibility": "PUBLIC"
    },
    {
      "pattern": "/.well-known/jwks.json",
      "methods": ["GET"],
      "visibility": "PUBLIC"
    }
  ]
}
